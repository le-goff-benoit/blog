<%= render "layouts/page_title" %>
<div class="container rounded" style="background:white; padding: 2%">
  <table class="table">
    <thead class="thead-dark rounded">
      <tr>
        <th scope="col">#</th>
        <th scope="col">Action</th>
        <th scope="col">Date</th>
      </tr>
    </thead>
    <tbody>
      <% @updates.each do |update| %>
        <tr>
          <td><%= update.id %></td>
          <td><%= update.title %></td>
          <td><%= update.created_at %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
  </div>
